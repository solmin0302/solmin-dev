{
  "name": "@solmin-dev/eslint-config-solmin",
  "version": "0.0.3",
  "description": "Eslint config for Solmin",
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:es; npm run prettier",
    "lint:fix": "npm run lint:es:fix; npm run prettier:fix",
    "lint:es": "eslint '**/*.js'",
    "lint:es:fix": "eslint '**/*.js' --fix",
    "prettier": "prettier '**/*' --check",
    "prettier:fix": "prettier '**/*' --write",
    "update-snapshot": "jest -u",
    "test:watch": "jest --watch",
    "test": "jest --passWithNoTests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/eslint": "8.44.8",
    "@types/node": "^20",
    "eslint": "^8",
    "jest": "29.7.0"
  },
  "dependencies": {
    "@solmin-dev/prettier-config-solmin": "workspace:*",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-config-standard-react": "13.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react-hooks": "4.6.0"
  }
}
